{
  "name": "youtube-videos-tracker",
  "version": "1.0.0",
  "description": "Video for tracking the status of my various short films in production.",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client/YouTube-Video-Tracker && npm run dev\"",
    "install-server": "cd server && npm install",
    "install-client": "cd client/YouTube-Video-Tracker && npm install",
    "setup": "npm install && npm run install-server && npm run install-client",
    "build": "cd client/YouTube-Video-Tracker && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "@apollo/client": "^3.12.4",
    "@apollo/server": "^4.11.0",
    "@emailjs/browser": "^4.4.1",
    "apollo-server-express": "^3.13.0",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.9.4",
    "prop-types": "^15.8.1",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^7.4.0"
  }
}
